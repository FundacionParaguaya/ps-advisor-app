<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DashboardTab">
        <attr name="tabImage" format="reference"/>
        <attr name="tabCaption" format="reference"/>
        <attr name="showCaption" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="IndicatorCard">
        <attr name="indicator_image" format="string"/>
        <attr name="indicator_text" format="string"/>
