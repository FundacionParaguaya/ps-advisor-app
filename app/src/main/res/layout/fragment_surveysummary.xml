<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/surveysummary_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <org.fundacionparaguaya.advisorapp.viewcomponents.SurveySummary_Layout
        android:id="@+id/surveysummary_background"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        app:surveysummary_title="@string/surveysummary_backgroundtitle"/>

    <org.fundacionparaguaya.advisorapp.viewcomponents.SurveySummary_Layout
        android:id="@+id/surveysummary_indicators"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        app:surveysummary_title="@string/surveysummary_indicatortitle"/>


</LinearLayout>